# Anthropic API Key (required)
ANTHROPIC_API_KEY=your_api_key_here

# Model configuration
ANTHROPIC_MODEL=claude-sonnet-4-5-20250929

# Orchestration settings
MAX_PARALLEL_AGENTS=5
