{
  "name": "agentic-brand-builder",
  "version": "1.0.0",
  "description": "AI-powered brand intelligence system with 6-stage multi-agent orchestration for comprehensive brand strategy documents",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "orchestrate": "tsx src/cli.ts orchestrate",
    "analyze": "tsx src/cli.ts analyze",
    "health-check": "tsx scripts/health-check.ts",
    "detect-deep-review": "tsx scripts/orchestrator/deep-review-detector.ts",
    "review:deep": "tsx scripts/local/deep-review.ts",
    "review:deep:help": "tsx scripts/local/deep-review.ts --help"
  },
  "keywords": [
    "brand-intelligence",
    "ai-agents",
    "multi-agent-system",
    "brand-strategy",
    "orchestration",
    "claude-ai"
  ],
  "author": "Kalpesh Jaju",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "marked": "^14.1.3",
    "ora": "^8.1.0",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^23.5.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/pdf-parse": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
